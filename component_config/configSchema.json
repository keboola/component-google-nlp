{
    "title": "Configuration",
    "type": "object",
    "required":[
        "#API_key",
        "analysis_type"
    ],
    "properties":{
        "#API_key":{
            "title": "Google API Key",
            "format": "password",
            "type": "string",
            "minLength": 4,
            "description": "Please visit <a href='https://console.cloud.google.com/apis/credentials'>Google Cloud Console</a>"
        },
        "analysis_type":{
            "enum": [
                "sentiment",
                "entities",
                "syntax"
            ],
            "title": "Analysis Type",
            "type": "string",
            "default": "entities"
        }
    }
}